

@{
    ViewBag.Title = "Index";
}



<h2>Report View</h2>
<div><h4 align="center">@ViewBag.Message</h4>
</div>
<p id="demo"></p>
<script>
document.getElementById("demo").innerHTML = Date();
</script>
<table>
@foreach (var employee in ViewBag.Employee)
{
    if (@employee.LastName.Equals("Doe")) { 
    <tr>
        <td>@employee.LastName</td>
        <td>@employee.FirstName</td>
    </tr>
    <tr>
    <td>@employee.UserId</td>
    </tr>
    }

}
</table>

<hr />
<h4> 
    <form action="https://www.facebook.com/" method="get">
        Call Time
        <input type="date" name="sday"> to
        <input type="date" name="eday" />
        <input type="submit" value="Submit" />

    </form>
</h4>
<table class="table">
<tr>
<th>Date</th>
<th>Resolution Start Time</th>
<th>Resolution End Time</th>
<th>Total Time</th>
</tr>
@foreach(var report in ViewBag.Report) {
    if (report.UserId.Equals(2))
    {
<tr>
<td>@report.Date</td>
<td>@report.ResolutionStartTime</td>
<td>@report.ResolutionEndTime</td>
</tr>
    }

}
</table> 
<hr />
    <h4>
        <form action="action_page.php">
            Star Rating
            <input type="date" name="sday"> to
            <input type="date" name="eday" />

        </form>
    </h4>
    <table class="table">
        <tr>
            <th>Star Rating</th>
        </tr>
        @foreach (var report in ViewBag.Report)
        {
            if (report.UserId.Equals(2))
            {
            <tr>
                <td>@report.StarRating</td>
            </tr>
            }
        }
    </table> 
    <hr />
<table class="table">
<tr>
<th>Total Rating</th>
</tr>
<tr>
<td>3</td>
</tr>
</table>   
</div>






       